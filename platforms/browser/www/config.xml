<?xml version='1.0' encoding='utf-8'?>
<widget id="com.salmandfarzanegan" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>خیریه فرزانگان</name>
    <description>
    </description>
    <gap:plugin name="com.phonegap.plugins.barcodescanner" />
    <gap:plugin name="com.pushwoosh.plugins.pushwoosh" />
    <gap:plugin name="de.appplant.cordova.plugin.email-composer" />
    <gap:plugin name="nl.x-services.plugins.socialsharing" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <content src="index.html" />
    <access origin="*" />
    <gap:platform name="android">
        <allow-intent href="market:*" />
        <access origin="*" />
    </gap:platform>
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackupWebStorage" value="none" />
</widget>
